<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mobi.efficacious.ESmartDemo">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoesmarts"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/logoesmarts"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:icon,android:label,android:theme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="778c8e4ea7b696aa559ee17eba4355731cf6b463" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.activity.SplashScreen"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.activity.Login_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.dialogbox.Standard_division_dialog"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.dialogbox.Book_Details_dialog"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.dialogbox.Diary_image_dialogBox"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.dialogbox.image_zoom_dialog"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.dialogbox.Gallery_dialogBox"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.Tab.Admin_LeaveList"
            android:label="Leave List" />
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.MultiImages.activities.AlbumSelectActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name"
            android:theme="@style/MultipleImageSelectTheme">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.MultiImages.activities.ImageSelectActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name"
            android:theme="@style/MultipleImageSelectTheme">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.activity.Gmail_login"
            android:label="Gmail Verification" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.MultiImages.activities.MainImages" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.activity.Notifiacton" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.activity.MessageCenterActivity" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.activity.NoticeboardActivity" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.MultiImages.activities.Single_image" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.activity.IndividualChat" />
        <activity android:name="com.mobi.efficacious.ESmartDemo.MultiImages.activities.Three_Images" />
        <activity
            android:name="com.atom.mobilepaymentsdk.PayActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.mobi.efficacious.ESmartDemo.activity.Fee_Payment_History"
            android:label="Payment History" />
        <activity android:name=".activity.LibCategoryBooklist_fragment"
            android:label="Book List"/>

        <service android:name="com.mobi.efficacious.ESmartDemo.FCMServices.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
<!--        <service android:name="com.mobi.efficacious.ESmartDemo.FCMServices.MyFirebaseInstanceIdService">-->
<!--            <intent-filter>-->
<!--                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />-->
<!--            </intent-filter>-->
<!--        </service>-->
    </application>

</manifest>